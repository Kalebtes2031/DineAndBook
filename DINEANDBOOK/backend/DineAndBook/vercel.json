{
    "version": 2,
    "builds": [
      {
        "src": "DineAndBook/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.12.3" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "DineAndBook/wsgi.py"
      }
    ]
  }
  